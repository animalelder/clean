{
  "singleQuote": false,
  "semi": true,
  "plugins": [
    "prettier-plugin-tailwindcss",
    "@ianvs/prettier-plugin-sort-imports"
  ],
  "importOrder": [
    "",
    "^next$",
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^(@/app|@/components|@/hooks|@/lib)(/.*)$",
    "^[.]"
  ],
  "importOrderParserPlugins": ["javascript", "jsx"]
}
